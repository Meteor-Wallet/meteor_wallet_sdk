{
  "name": "meteor-sdk-v1-test-web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "bun run clear-vite-cache && react-router dev --force",
    "watch-meteor-script": "chokidar \"../../near-connect/meteor-near-connect.js\" -c \"cpy ../../near-connect/meteor-near-connect.js public\"",
    "start": "react-router-serve ./build/server/index.js",
    "clear-vite-cache": "trash '**/.vite'",
    "type-check": "react-router typegen && tsc"
  },
  "dependencies": {
    "@meteorwallet/sdk": "workspace:*",
    "@scure/base": "2.0.0",
    "@near-js/keystores-browser": "2.2.5",
    "@near-js/crypto": "2.2.5",
    "@near-js/utils": "2.2.5",
    "@near-js/transactions": "2.2.5",
    "@near-js/types": "2.2.5",
    "@near-wallet-selector/core": "^9.3.0",
    "@near-wallet-selector/react-hook": "^9.3.0",
    "@react-router/node": "^7.8.0",
    "@react-router/serve": "^7.8.0",
    "@tanstack/react-query": "^5.85.3",
    "buffer": "^6.0.3",
    "isbot": "^5.1.27",
    "near-api-js": "^6.2.5",
    "use-immer": "^0.11.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "^7.8.0",
    "@hot-labs/near-connect": "0.9.0",
    "usehooks-ts": "3.1.1"
  },
  "devDependencies": {
    "@react-router/dev": "^7.8.0",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "sass-embedded": "1.97.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "trash-cli": "^5.0.0",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vite-plugin-devtools-json": "^1.0.0",
    "vite-plugin-watch-node-modules": "0.5.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-web-components-hmr": "0.1.3",
    "chokidar-cli": "^3.0.0",
    "cpy-cli": "^6.0.0"
  }
}
