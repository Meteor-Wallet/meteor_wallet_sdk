{
  "name": "@meteorwallet/meteor-connect-ui",
  "version": "1.1.0",
  "description": "The UI renderer for the Meteor Connect SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "./dist",
    "./src",
    "./package.json"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check-watch": "tsc --noEmit --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Meteor-Wallet/meteor_wallet_sdk.git"
  },
  "dependencies": {
    "@meteorwallet/sdk": "1.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "private": false
}
